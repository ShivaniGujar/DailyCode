package JavaDemo;

public class Switch8 {
	public static void main(String[] args) {
		byte a=1;
		switch(a+1) {
		case 1: System.out.println("Hi from case 1");
				break;
		
		case 'b': System.out.println("Hi from case 3");
				break;
		
		case 128: System.out.println("Hi from case 5");
				break;
		
		}
	}

}
