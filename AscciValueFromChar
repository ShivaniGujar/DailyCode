package JavaDemo;

public class AscciValueFromChar {

	public static void main(String[] args) {
		int asciiValue=65;
		for(char ch='A';ch<='Z';ch++) {
			/*System.out.println((ch+0)+"-->"+ch+" ");
			System.out.println((int)ch+"-->"+ch);* these 2 line same*/
			System.out.println(asciiValue++  +"-->"+ch);
			
		}

	}

}
