/*WAP to find smallest number among 2*/
package JavaDemo;

import java.util.Scanner;
public class RelationalOpAssign2 {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.println("Enter 2 number:");
		int num1=sc.nextInt();
		int num2=sc.nextInt();
		int min = (num1 > num2) ? num2 : num1;
		System.out.print(min + " smallest among the two number ");
	}

}
